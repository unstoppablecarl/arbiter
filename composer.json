{
    "name": "unstoppablecarl/arbiter",
    "version": "0.0.1",
    "description": "Manage Laravel User abilities that target Users",
    "type": "library",
    "keywords": [
        "laravel",
        "auth",
        "gate",
        "policy"
    ],
    "homepage": "https://github.com/unstoppablecarl/arbiter",
    "license": "MIT",
    "authors": [
        {
            "name": "Carl Olsen",
            "email": "unstoppablecarlolsen@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/contracts": ">=5.4",
        "illuminate/support": ">=5.4",
    },
    "require-dev": {
        "illuminate/auth": ">=5.4",
        "phpunit/phpunit": "^5.7 || ^6",
        "satooshi/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "UnstoppableCarl\\Arbiter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UnstoppableCarl\\Arbiter\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/unstoppablecarl/arbiter/issues",
        "source": "https://github.com/unstoppablecarl/arbiter"
    },
    "suggest": {
        "unstoppablecarl/gate-crasher": "Safer Laravel superuser auth"
    },
    "scripts": {
        "phpunit": "phpunit",
        "phpcs": "phpcs src tests --standard=psr2 -sp --colors",
        "test": [
            "@phpunit",
            "@phpcs"
        ]
    }
}